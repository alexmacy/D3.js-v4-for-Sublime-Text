						"command": "insert_snippet", "args": {"contents": "d3.randomUniform(${1:[min, ]}${2:[max]})"}
					}, {

						"caption": "-"

					}, {
						//d3.randomNormal - from a normal distribution.
						"caption": "d3.randomNormal([mu][, sigma])",
