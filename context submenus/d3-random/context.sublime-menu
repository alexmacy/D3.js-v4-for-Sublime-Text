					}, {
						//d3.randomBates - from a Bates distribution.
						"caption": "d3.randomBates(n)",
						"command": "insert_snippet", "args": {"contents": "d3.randomBates(${1:n})"}
					}, {

						"caption": "-"

					}, {
						//d3.randomIrwinHall - from an Irwinâ€“Hall distribution.
						"caption": "d3.randomIrwinHall(n)",
						"command": "insert_snippet", "args": {"contents": "d3.randomIrwinHall(${1:n})"}
					}, {

						"caption": "-"

					}, {
						//d3.randomExponential - from an exponential distribution.
						"caption": "d3.randomExponential(lambda)",
