						//d3.easeSinIn - sinusoidal easing.
						"caption": "d3.easeSinIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeSinIn"}
					}, {
						//d3.easeSinOut - reverse sinusoidal easing.
						"caption": "d3.easeSinOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeSinOut"}
					}, {

						"caption": "-",

					}, {
						//d3.easeExp - an alias for easeExpInOut.
						"caption": "d3.easeExp",
						"command": "insert_snippet", "args": {"contents": "d3.easeExp"}
					}, {
						//d3.easeExpInOut - symmetric exponential easing.
						"caption": "d3.easeExpInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeExpInOut"}
