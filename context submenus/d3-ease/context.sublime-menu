[
	{
		"id": "D3.js",
		"caption": "D3.js",
		"children": 
		[
			//////////////////////////////////////////////////////////////////////
			//Easings (d3-ease)
			//Easing functions for smooth animation
			//////////////////////////////////////////////////////////////////////

			{
			  "caption": "Easings (d3-ease)",
			  "children":
			  [
					{
						//ease - ease the given normalized time.
						"caption": "ease(t)",
						"command": "insert_snippet", "args": {"contents": "${1:ease}(${2:t})"}
					}, {

						"caption": "-",

					}, {
						//d3.easeLinear - linear easing; the identity function.
						"caption": "d3.easeLinear",
						"command": "insert_snippet", "args": {"contents": "d3.easeLinear"}
					}, {

						"caption": "-",

					}, {
						//d3.easePoly - polynomial easing; raises time to the given power.
						"caption": "d3.easePoly",
						"command": "insert_snippet", "args": {"contents": "d3.easePoly"}
					}, {
						//d3.easePolyInOut - symmetric polynomial easing.
						"caption": "d3.easePolyInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easePolyInOut"}
					}, {
						//d3.easePolyIn - polynomial easing; raises time to the given power.
						"caption": "d3.easePolyIn",
						"command": "insert_snippet", "args": {"contents": "d3.easePolyIn"}
					}, {
						//d3.easePolyOut - reverse polynomial easing.
						"caption": "d3.easePolyOut",
						"command": "insert_snippet", "args": {"contents": "d3.easePolyOut"}
					}, {
						//poly.exponent - specify the polynomial exponent.
						"caption": "    .exponent(e)",
						"command": "insert_snippet", "args": {"contents": ".exponent(${1:e})"}
					}, {

						"caption": "-",

					}, {
						//d3.easeQuad - an alias for easeQuadInOut.
						"caption": "d3.easeQuad",
						"command": "insert_snippet", "args": {"contents": "d3.easeQuad"}
					}, {
						//d3.easeQuadInOut - symmetric quadratic easing.
						"caption": "d3.easeQuadInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeQuadInOut"}
					}, {
						//d3.easeQuadIn - quadratic easing; squares time.
						"caption": "d3.easeQuadIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeQuadIn"}
					}, {
						//d3.easeQuadOut - reverse quadratic easing.
						"caption": "d3.easeQuadOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeQuadOut"}
					}, {

						"caption": "-",

					}, {
						//d3.easeCubic - an alias for easeCubicInOut.
						"caption": "d3.easeCubic",
						"command": "insert_snippet", "args": {"contents": "d3.easeCubic"}
					}, {
						//d3.easeCubicInOut - symmetric cubic easing.
						"caption": "d3.easeCubicInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeCubicInOut"}
					}, {
						//d3.easeCubicIn - cubic easing; cubes time.
						"caption": "d3.easeCubicIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeCubicIn"}
					}, {
						//d3.easeCubicOut - reverse cubic easing.
						"caption": "d3.easeCubicOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeCubicOut"}
					}, {

						"caption": "-",

					}, {
						//d3.easeSin - an alias for easeSinInOut.
						"caption": "d3.easeSin",
						"command": "insert_snippet", "args": {"contents": "d3.easeSin"}
