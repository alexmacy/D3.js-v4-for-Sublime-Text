						//d3.easeExpOut - reverse exponential easing.
						"caption": "d3.easeExpOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeExpOut"}
					}, {

						"caption": "-",

					}, {
						//d3.easeCircle - an alias for easeCircleInOut.
						"caption": "d3.easeCircle",
						"command": "insert_snippet", "args": {"contents": "d3.easeCircle"}
					}, {
						//d3.easeCircleInOut - symmetric circular easing.
						"caption": "d3.easeCircleInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeCircleInOut"}
					}, {
						//d3.easeCircleIn - circular easing.
						"caption": "d3.easeCircleIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeCircleIn"}
					}, {
						//d3.easeCircleOut - reverse circular easing.
						"caption": "d3.easeCircleOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeCircleOut"}
					}, {

						"caption": "-",

					}, {
						//d3.easeElastic - an alias for easeElasticOut.
						"caption": "d3.easeElastic",
						"command": "insert_snippet", "args": {"contents": "d3.easeElastic"}
					}, {
						//d3.easeElasticInOut - symmetric elastic easing.
						"caption": "d3.easeElasticInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeElasticInOut"}
					}, {
						//d3.easeElasticIn - elastic easing, like a rubber band.
						"caption": "d3.easeElasticIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeElasticIn"}
					}, {
						//d3.easeElasticOut - reverse elastic easing.
						"caption": "d3.easeElasticOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeElasticOut"}
					}, {
						//elastic.amplitude - specify the elastic amplitude.
						"caption": "    .amplitude(a)",
						"command": "insert_snippet", "args": {"contents": ".amplitude({1:a})"}
					}, {
						//elastic.period - specify the elastic period.
						"caption": "    .period(p)",
						"command": "insert_snippet", "args": {"contents": ".period({1:p})"}
					}, {

						"caption": "-",

					}, {
						//d3.easeBack - an alias for easeBackInOut.
						"caption": "d3.easeBack",
						"command": "insert_snippet", "args": {"contents": "d3.easeBack"}
					}, {
						//d3.easeBackInOut - symmetric anticipatory easing.
						"caption": "d3.easeBackInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeBackInOut"}
					}, {
						//d3.easeBackIn - anticipatory easing, like a dancer bending his knees before jumping.
						"caption": "d3.easeBackIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeBackIn"}
					}, {
						//d3.easeBackOut - reverse anticipatory easing.
						"caption": "d3.easeBackOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeBackOut"}
					}, {
						//back.overshoot - specify the amount of overshoot.
						"caption": "    .overshoot(s)",
						"command": "insert_snippet", "args": {"contents": ".overshoot({1:s})"}
					}, {

						"caption": "-",

					}, {
						//d3.easeBounce - an alias for easeBounceOut.
						"caption": "d3.easeBounce",
						"command": "insert_snippet", "args": {"contents": "d3.easeBounce"}
