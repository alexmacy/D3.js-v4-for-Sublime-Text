						"caption": "d3.easeBounceInOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeBounceInOut"}
					}, {
						//d3.easeBounceIn - bounce easing, like a rubber ball.
						"caption": "d3.easeBounceIn",
						"command": "insert_snippet", "args": {"contents": "d3.easeBounceIn"}
					}, {
						//d3.easeBounceOut - reverse bounce easing.
						"caption": "d3.easeBounceOut",
						"command": "insert_snippet", "args": {"contents": "d3.easeBounceOut"}
					}
				]
			}
		]
	}
]